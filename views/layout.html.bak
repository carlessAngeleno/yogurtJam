  <div id="main_container">
    <div id="map_container">   
      <div id="map_canvas"></div>
    </div>
    <div id="interface">
        <div id="content_display"><div id="player"></div></div>
        <div id="memory_display">
          <div id = "search_tab">
            <h4>Search Memories</h4>
              <p>What does your song mean to other people? </p>
            <div class="search_tab_sub">                  
                <form id="search_form">                                       
                    <fieldset>                        
                        <div class="control-group">
                            <input id = "title_search" name="title" placeholder="title (e.g. smells like teen spirit)" type="text"/>
                        </div>
                        <div class="control-group">
                            <input id="artist_search" name="artist" placeholder="artist (e.g. nirvana)" type="text"/>
                        </div>
                        <div class="control-group">
                            <button id = "search_button" type="submit" onClick="onClickReset()">search for memories</button>                               
                        </div>
                    </fieldset>            
                </form>	           
            </div>      
          </div>
          
          <div id = "share_tab">
                <h4>Share Memories</h4>
                <!-- first, ask which song -->
                <div class="share_tab_sub" id="share_tab_select_video">
                    <form>
                        <div class="control-group">
                            <input id = "title_share" name="title" placeholder="title (e.g. the scientist)" type="text"/>
                        </div>
                        <div class="control-group">
                            <input id="artist_share" name="artist" placeholder="artist (e.g. coldplay)" type="text"/>
                        </div>
                        <!-- clicking on button pulls up next stage --> 
                        <div class="control-group">
                            <button id = "youtube_search_button" onClick=
                                "document.getElementById('select_video').style.display = 'block';
                                document.getElementById('share_tab_final_submit').style.display = 'none';"> find videos for it </button> 
                        </div>
                    </form>       
                        <!-- pick which YT video -->
                        <button id="select_video" style="display:none" onClick=
                            "document.getElementById('share_tab_select_location').style.display = 'block';
                            document.getElementById('share_tab_select_video').style.display = 'none';
                            document.getElementById('share_tab_final_submit').style.display = 'none';"> choose video currently playing </button>
                        <!-- container for YT player -->
                        <div id="video_section_share">                             
                            <div id="player_container_share"> </div>                                           
                            <!-- GSVideoBar (interactive search result that plays YT videos when you click on thumbnails)-->
                            <div>
                                <div id="videoBar"></div>                               
                            </div>                  
                        </div>               
                </div> 
               
                                             
          </div>          
        </div>
    </div>       
  </div> 
  
  
  
  
  
  

    <!-- right_panel: -->
    <div class="span4" id="right_panel" style="float:right">   
        <!--
        <ul class="nav nav-pills" style="float:right;">
            <li><a href="logout.php"><strong>Log Out</strong></a></li>
        </ul>
        -->
        <ul class="nav nav-tabs" id="myTab">
            <li class="active" id="header_search_tab">
				<a href="#search_tab">search memories</a>
			</li>
            <li id="header_share_tab">
				<a href="#share_tab">share memories</a>
			</li>
        </ul>
        
        <!-- content to be shown when each tab is clicked --> 
        <div class="tab-content">
        
            <!-- 1. search tab -->
            <div class="tab-pane active" id="search_tab_old">              
                <form id="search_form"> 
                    <fieldset>
                        <div class="control-group">
                            <input id = "title_search" name="title" placeholder="title (e.g. smells like teen spirit)" type="text"/>
                        </div>
                        <div class="control-group">
                            <input id="artist_search" name="artist" placeholder="artist (e.g. nirvana)" type="text"/>
                        </div>
                        <div class="control-group">
                            <button id = "search_button" type="submit" onClick="onClickReset()">search for memories</button>   
                		<!--	<button name="name" onClick="ajax('yogurtjam/default/echo', ['name'], ':eval')" /> -->
                            
                        </div>
                    </fieldset>
                </form>	


                <!-- container for YT player on the search tab (note: different obj than the one on the "share" tab later) -->
                <div id="player" style="display:none"></div>
            </div>            
            
            <!-- 2. share tab -->          
            <div class="tab-pane" id="share_tab_old">                
                
                    
                    
                <!-- secondly, choose location associated with memory -->
                <div class="share_tab_sub" id="share_tab_select_location" style="display:none">
                    <p> Enter location associated with memory </p>                    
                    <!-- search box for location -->      
                    <input id="autocomplete" placeholder="start typing and click on drop-down menu" type="text">  
                    <br/>
                    <button id="select_place_button" onClick=
                                "document.getElementById('share_tab_final_submit').style.display = 'block';
                                document.getElementById('share_tab_select_location').style.display = 'none';"> yup - this is the place! </button>                                            
                    </form>            
                </div>                
                
                <!-- finally, details about the memory -->
                <form class="share_tab_sub" id="share_tab_final_submit" style="display:none">
                    <p> Finally, tell us what the song reminds you of! </p>                
                    <div class="control-group">
                        <input id="year_share" name="year" placeholder="year" type="text"/>
                    </div>
                    <div class="control-group">
                        <input id="month_share" name="month" placeholder="month (in numbers)" type="text"/>
                    </div>                            
                    <div class="control-group">
                        <TEXTAREA id="story_share" name="story" placeholder="reminds me of..." ROWS=10 COLS=30></TEXTAREA>
                    </div>           
                    <div class="control-group">
                        <input id="tag1_share" name="tag1" placeholder="tags (e.g. trip, friend)" type="text"/>
                    </div>                                                                                  
                    <div class="control-group">
                        <button id = "share_button" onClick=
                                "document.getElementById('share_tab_select_video').style.display = 'block';
                                "document.getElementById('select_video').style.display = 'none';
                                "document.getElementById('video_selection_share').style.display = 'none';
                                document.getElementById('share_tab_final_submit').style.display = 'none';"
                        type="submit">share memory</button> 
                    </div>                
                </form>   
            </div> 
        </div>      <!-- end of tab-menu content -->  
        
    </div>      <!-- end of right panel -->   
  
  
